  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "SimMain - local",
      "request": "launch",
      "mainClass": "frc.robot.simulator.SimMain",
      "vmArgs": "-Djava.library.path=build/tmp/jniExtractDir",
      "env": {
        "DYLD_LIBRARY_PATH": "build/tmp/jniExtractDir"
      }
    },
    {
      "type": "wpilib",
      "name": "WPILib Desktop Debug",
      "request": "launch",
      "desktop": true,
    },
    {
      "type": "wpilib",
      "name": "WPILib roboRIO Debug",
      "request": "launch",
      "desktop": false,
    }
  ]
}
